{
  "name": "starter",
  "version": "0.1.0",
  "description": "A minimal, lightweight and mobile-first template.",
  "main": "n/a",
  "scripts": {
    "develop": "npm run clean && gatsby develop",
    "build": "npm run clean && gatsby build --prefix-paths",
